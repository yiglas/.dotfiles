{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#98C379",
                    "properties": {
                        "template": "\u279c"
                    },
                    "style": "plain",
                    "type": "text"
                },
                {
                    "foreground": "#56B6C2",
                    "properties": {
                        "style": "folder",
                        "template": " {{ .Path }} "
                    },
                    "style": "plain",
                    "type": "path"
                },
                {
                    "foreground": "#D0666F",
                    "properties": {
                        "branch_icon": "",
                        "template": "<#5FAAE8>(</>{{ .HEAD }}<#5FAAE8>)</>"
                    },
                    "style": "plain",
                    "type": "git"
                },
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "#ffeb3b",
                    "properties": {
                        "fetch_status": true,
                        "template": "{{ if or (.Working.Changed) (.Staging.Changed) }} âœ—{{ end }}"
                    }
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 1
}
